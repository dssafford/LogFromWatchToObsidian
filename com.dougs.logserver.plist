<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.dougs.logserver</string>

    <key>UserName</key>
    <string>dougs</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/dougs/.local/bin/uv</string>
        <string>run</string>
        <string>--project</string>
        <string>/Users/dougs/PycharmProjects/LogFromWatch</string>
        <string>python</string>
        <string>/Users/dougs/PycharmProjects/LogFromWatch/server.py</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/tmp/logserver.stdout.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/logserver.stderr.log</string>

    <key>WorkingDirectory</key>
    <string>/Users/dougs/PycharmProjects/LogFromWatch</string>
</dict>
</plist>
